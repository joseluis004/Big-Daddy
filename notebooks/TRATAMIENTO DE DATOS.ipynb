    "cols_anecdoticas = [\"MARITAL_STATUS\", \"FAMILY_SIZE\"]\n",
    "df_clients = df_clients.fillna(\"UNKNOWN\", subset=cols_anecdoticas)\n",
    "\n",
    "print(\"Limpieza completada.\")"
