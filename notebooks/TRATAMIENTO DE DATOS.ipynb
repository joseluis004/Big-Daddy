    "    ]\n",
    "\n",
    "# Borrado seguro (solo las que existan)\n",
    "cols_finales_borrar = [c for c in cols_a_borrar if c in df_master.columns]\n",
    "df_master = df_master.drop(*cols_finales_borrar)\n",
    "\n",
