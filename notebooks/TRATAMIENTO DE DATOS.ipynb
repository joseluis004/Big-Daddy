    "print(f\"Uniendo tablas por {col_id}...\")\n",
    "df_master = df_clients.join(df_behav_dedup, on=col_id, how=\"left\")\n",
    "\n",
    "# --- C. INTEGRIDAD ---\n",
    "filas_clientes = df_clients.count()\n",
